<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="org.sk.entity.Entity" table="sk_example_table" schema="public" catalog="sk_example_db">
        <id name="id">
            <column name="id" sql-type="integer"/>
        </id>
        <property name="obj">
            <column name="obj" sql-type="jsonb"/>
        </property>
    </class>
</hibernate-mapping>